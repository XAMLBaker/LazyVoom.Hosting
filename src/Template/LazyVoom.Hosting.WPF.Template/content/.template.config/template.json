{
  "$schema": "http://json.schemastore.org/template",
  "author": "LukeWire",
  "description": "LazyVoom.Hosting WPF Application Project",
  "classifications": [ "Common", "WPF", "LazyVoom" ],
  "identity": "lazyvoomhosting-wpf",
  "name": "WPF Application (LazyVoomHosting App)",
  "shortName": "wpf-lazyvoom-hosting",
  "sourceName": "LazyVoomHosting.WPF.App",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "icon": "icon.png",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target net6.0-windows",
          "displayName": ".NET 6.0"
        },
        {
          "choice": "net7.0",
          "description": "Target net7.0-windows",
          "displayName": ".NET 7.0"
        },
        {
          "choice": "net8.0",
          "description": "Target net8.0-windows",
          "displayName": ".NET 8.0"
        },
        {
          "choice": "net9.0",
          "description": "Target net9.0-windows",
          "displayName": ".NET 9.0"
        }
      ],
      "replaces": "net8.0-windows",
      "defaultValue": "net8.0-windows"
    }
  }
}